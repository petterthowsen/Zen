const FIB_NUM = 10

var total_n = 0

func fib(n:int) : int {
    total_n += 1

    if n <= 2 {
        return n
    }
    return fib(n - 1) + fib(n - 2)
}

print "Getting " + FIB_NUM + "th fib number...\n"

var start = time()
fib(FIB_NUM)
var end = time()
var elapsed = end - start

print "Time: " + elapsed + "ms\n"

print "called fib " + total_n + " times\n"